class Solution {
public:
    int minimumRounds(vector<int>& tasks) {
        unordered_map<int,int> mp;

        for(int i = 0;i < tasks.size();i++){
            mp[tasks[i]]++;
        }
        int minRound = 0;
        for(auto x : mp){
            if(x.second == 1) return -1;

            if(x.second%3 == 0){
                minRound += x.second/3;
            }
            else{
                minRound += x.second/3 + 1;
            }
        }
        return minRound;
    }
};